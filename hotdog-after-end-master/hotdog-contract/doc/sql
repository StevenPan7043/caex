SET NAMES utf8mb4;
SET FOREIGN_KEY_CHECKS = 0;

-- ----------------------------
-- Table structure for c_cash_usdt_logs
-- ----------------------------
DROP TABLE IF EXISTS `c_cash_usdt_logs`;
CREATE TABLE `c_cash_usdt_logs`  (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `cid` int(11) NULL DEFAULT NULL,
  `fromaddr` varchar(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT NULL COMMENT '提供者地址',
  `money` decimal(18, 6) NULL DEFAULT 0.000000 COMMENT '提币数量',
  `tax` decimal(18, 6) NULL DEFAULT 0.000000 COMMENT '手续费',
  `realmoney` decimal(18, 6) NULL DEFAULT 0.000000 COMMENT '实际到账',
  `toaddr` varchar(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT NULL COMMENT '接收者',
  `status` int(11) NULL DEFAULT 1 COMMENT '1待审核 2已通过 3已驳回',
  `hashcode` varchar(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT '' COMMENT '交易单号',
  `createtime` datetime(0) NULL DEFAULT CURRENT_TIMESTAMP,
  `checktime` datetime(0) NULL DEFAULT NULL COMMENT '审核时间',
  `checkid` int(11) NULL DEFAULT NULL COMMENT '审核人',
  `dztime` datetime(0) NULL DEFAULT NULL COMMENT '对账时间',
  `dzstatus` int(1) NULL DEFAULT -1 COMMENT '-1未对账  0正常交易，1需要重审',
  PRIMARY KEY (`id`) USING BTREE
) ENGINE = InnoDB AUTO_INCREMENT = 1 CHARACTER SET = utf8mb4 COLLATE = utf8mb4_general_ci ROW_FORMAT = Compact;

-- ----------------------------
-- Table structure for c_contract_order
-- ----------------------------
DROP TABLE IF EXISTS `c_contract_order`;
CREATE TABLE `c_contract_order`  (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `ordercode` varchar(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NOT NULL COMMENT '订单号',
  `cid` int(11) NOT NULL COMMENT '用户id',
  `type` int(1) NOT NULL COMMENT '1开多 2开空',
  `realmoney` decimal(18, 4) NULL DEFAULT 0.0000 COMMENT '实际花费金额',
  `gearing` int(11) NULL DEFAULT 0 COMMENT '杠杆倍数',
  `money` decimal(20, 4) NULL DEFAULT 0.0000 COMMENT '杠杆放大金额',
  `buyprice` decimal(20, 4) NULL DEFAULT 0.0000 COMMENT '买入价 市值',
  `coin` varchar(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT NULL COMMENT '货币类型 见coin表 symbol字段',
  `coinnum` decimal(20, 4) NULL DEFAULT 0.0000 COMMENT '持仓量',
  `stopprice` decimal(20, 4) NULL DEFAULT 0.0000 COMMENT '平仓价 市值',
  `createtime` datetime(0) NULL DEFAULT CURRENT_TIMESTAMP COMMENT '持仓时间',
  `stoptime` datetime(0) NULL DEFAULT NULL COMMENT '平仓时间',
  `tax` decimal(18, 4) NULL DEFAULT 0.0000 COMMENT '手续费',
  `rates` decimal(18, 4) NULL DEFAULT 0.0000 COMMENT '利息',
  `status` int(11) NULL DEFAULT 1 COMMENT '1持仓中 2已完成',
  `automoney` decimal(20, 4) NULL DEFAULT 0.0000 COMMENT '自动爆仓价',
  `settleflag` int(11) NULL DEFAULT -1 COMMENT '-1未结算 1已结算',
  `outtype` int(1) NULL DEFAULT 1 COMMENT '1手动平仓 2自动爆仓',
  `stopwin` decimal(20, 4) NULL DEFAULT 0.0000 COMMENT '止盈价格',
  `stopdonat` decimal(20, 4) NULL DEFAULT 0.0000 COMMENT '止损价格',
  `spread` decimal(18, 4) NULL DEFAULT 0.0000 COMMENT '点差比例',
  `spreadmoney` decimal(18, 4) NULL DEFAULT 0.0000 COMMENT '点差金额',
  `remark` varchar(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT '' COMMENT '备注',
  `ordertype` int(11) NULL DEFAULT 1 COMMENT '1合约订单  2逐仓订单',
  PRIMARY KEY (`id`) USING BTREE,
  INDEX `cid`(`cid`) USING BTREE,
  INDEX `ordercode`(`ordercode`) USING BTREE,
  INDEX `coin`(`coin`) USING BTREE
) ENGINE = InnoDB AUTO_INCREMENT = 1 CHARACTER SET = utf8mb4 COLLATE = utf8mb4_general_ci ROW_FORMAT = Compact;

-- ----------------------------
-- Table structure for c_customer
-- ----------------------------
DROP TABLE IF EXISTS `c_customer`;
CREATE TABLE `c_customer`  (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `phone` varchar(30) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT NULL COMMENT '手机号',
  `password` varchar(100) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT NULL COMMENT '密码',
  `payword` varchar(100) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT NULL COMMENT '交易密码',
  `invitationcode` varchar(20) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT NULL COMMENT '邀请码',
  `realname` varchar(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT NULL COMMENT '真实名称',
  `idcard` varchar(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT NULL COMMENT '身份证',
  `cardurls` varchar(300) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT '' COMMENT '身份证图片',
  `levelid` int(2) NULL DEFAULT 1 COMMENT '等级id',
  `createtime` datetime(0) NULL DEFAULT CURRENT_TIMESTAMP,
  `status` int(1) NULL DEFAULT 0 COMMENT '0有效 -1无效',
  `parentid` int(11) NULL DEFAULT NULL COMMENT '推荐人',
  `lastid` int(11) NULL DEFAULT NULL COMMENT '接点人',
  `layer` int(11) NULL DEFAULT 1 COMMENT '推荐代数',
  `pushgenes` text CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL COMMENT '推荐节点网',
  `qrurl` varchar(150) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT NULL COMMENT '分享二维码',
  `isvalid` int(11) NULL DEFAULT -1 COMMENT '-1未激活  0激活',
  `authflag` int(1) NULL DEFAULT 1 COMMENT '1未认证 2审核中 3审核通过',
  `userid` int(11) NULL DEFAULT NULL COMMENT '操作员id',
  `salesman` int(11) NULL DEFAULT NULL COMMENT '业务员id',
  `cancash` int(11) NULL DEFAULT 0 COMMENT '是否可以提现 0可以 -1不行',
  `salesflag` int(11) NULL DEFAULT -1 COMMENT '0是业务员 -1不是业务员',
  `identity` int(11) NULL DEFAULT 1 COMMENT '1会员 2业务员 3模拟账号',
  PRIMARY KEY (`id`) USING BTREE,
  INDEX `phone`(`phone`) USING BTREE,
  INDEX `invitationcode`(`invitationcode`) USING BTREE
) ENGINE = InnoDB AUTO_INCREMENT = 2 CHARACTER SET = utf8mb4 COLLATE = utf8mb4_general_ci COMMENT = '会员信息表' ROW_FORMAT = Compact;

-- ----------------------------
-- Table structure for c_entrust_order
-- ----------------------------
DROP TABLE IF EXISTS `c_entrust_order`;
CREATE TABLE `c_entrust_order`  (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `ordercode` varchar(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NOT NULL COMMENT '订单号',
  `cid` int(11) NOT NULL COMMENT '用户id',
  `type` int(1) NOT NULL COMMENT '1开多 2开空',
  `realmoney` decimal(18, 4) NULL DEFAULT 0.0000 COMMENT '实际花费金额',
  `gearing` int(11) NULL DEFAULT 0 COMMENT '杠杆倍数',
  `money` decimal(20, 4) NULL DEFAULT 0.0000 COMMENT '杠杆放大金额',
  `price` decimal(20, 4) NULL DEFAULT 0.0000 COMMENT '委托单价',
  `tax` decimal(18, 4) NULL DEFAULT 0.0000 COMMENT '手续费',
  `coin` varchar(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT NULL COMMENT '货币类型 见coin表',
  `coinnum` decimal(20, 4) NULL DEFAULT 0.0000 COMMENT '持仓量',
  `createtime` datetime(0) NULL DEFAULT CURRENT_TIMESTAMP COMMENT '委托时间',
  `status` int(11) NULL DEFAULT 1 COMMENT '1委托中 2委托成功 3已取消',
  `successtime` datetime(0) NULL DEFAULT NULL COMMENT '成功时间',
  `canceltime` datetime(0) NULL DEFAULT NULL COMMENT '取消时间',
  PRIMARY KEY (`id`) USING BTREE,
  INDEX `ordercode`(`ordercode`) USING BTREE,
  INDEX `cid`(`cid`) USING BTREE
) ENGINE = InnoDB AUTO_INCREMENT = 1 CHARACTER SET = utf8mb4 COLLATE = utf8mb4_general_ci ROW_FORMAT = Compact;

