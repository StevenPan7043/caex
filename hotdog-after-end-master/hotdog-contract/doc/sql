-- ----------------------------
-- Table structure for mt_clique_roles
-- ----------------------------
DROP TABLE IF EXISTS `mt_clique_roles`;
CREATE TABLE `mt_clique_roles`  (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT NULL COMMENT '角色名称',
  `menuids` text CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL COMMENT '角色菜单权限逗号分隔字符串',
  `status` smallint(1) NULL DEFAULT 0 COMMENT '0有效 -1无效',
  `createtime` datetime(0) NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
  `type` int(1) NULL DEFAULT 1 COMMENT '1总平台 2国家区域  3地区分公司 4国家',
  `targetid` int(11) NULL DEFAULT NULL COMMENT '目标id',
  PRIMARY KEY (`id`) USING BTREE
) ENGINE = InnoDB AUTO_INCREMENT = 14 CHARACTER SET = utf8mb4 COLLATE = utf8mb4_general_ci COMMENT = '集团角色表' ROW_FORMAT = Compact;

