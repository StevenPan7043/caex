-- ----------------------------
-- Table structure for c_zc_order
-- ----------------------------
DROP TABLE IF EXISTS `c_zc_order`;
CREATE TABLE `c_zc_order`  (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `cid` int(11) NULL DEFAULT NULL COMMENT '用户id',
  `ordercode` varchar(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT NULL COMMENT '订单号',
  `type` int(1) NULL DEFAULT 1 COMMENT '1开多 2开空',
  `capital` decimal(18, 6) NULL DEFAULT 0.000000 COMMENT '投入本金',
  `tax` decimal(18, 6) NULL DEFAULT 0.000000 COMMENT '手续费',
  `buyprice` decimal(18, 6) NULL DEFAULT 0.000000 COMMENT '买入价',
  `stopprice` decimal(18, 6) NULL DEFAULT 0.000000 COMMENT '平仓价',
  `createtime` datetime(0) NULL DEFAULT CURRENT_TIMESTAMP COMMENT '开仓时间',
  `coin` varchar(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT NULL COMMENT '货币对应coins表symbol字段',
  `stoptime` datetime(0) NULL DEFAULT NULL COMMENT '平仓时间',
  `reward` decimal(18, 6) NULL DEFAULT 0.000000 COMMENT '实际收益 可以是负数 包含本金',
  `zcprice` decimal(18, 4) NULL DEFAULT NULL COMMENT '逐仓理论爆仓金额',
  `zcscale` decimal(18, 4) NULL DEFAULT 0.0000 COMMENT '逐仓理论奖励倍数',
  `remark` varchar(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT NULL COMMENT '描述',
  `stopwin` decimal(18, 6) NULL DEFAULT 0.000000 COMMENT '止盈',
  `stopfail` decimal(18, 6) NULL DEFAULT 0.000000 COMMENT '止损',
  `status` int(1) NULL DEFAULT 1 COMMENT '1逐仓中 2已平仓',
  PRIMARY KEY (`id`) USING BTREE
) ENGINE = InnoDB AUTO_INCREMENT = 1 CHARACTER SET = utf8mb4 COLLATE = utf8mb4_general_ci ROW_FORMAT = Compact;

-- ----------------------------
-- Table structure for coins
-- ----------------------------
DROP TABLE IF EXISTS `coins`;
CREATE TABLE `coins`  (
  `coin` varchar(10) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NOT NULL COMMENT '货币名称',
  `name` varchar(20) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT NULL COMMENT '名称',
  `symbol` varchar(20) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT NULL COMMENT '市场id，交易对',
  `zbdepth` varchar(20) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT NULL COMMENT '中币合并深度',
  `openval` decimal(20, 8) NULL DEFAULT 0.00000000 COMMENT '上一次开盘价',
  `updatetime` datetime(0) NULL DEFAULT NULL,
  `beginspread` decimal(18, 4) NULL DEFAULT 0.0000 COMMENT '开始点差比例',
  `stopspread` decimal(18, 4) NULL DEFAULT 0.0000 COMMENT '截止点差比例',
  `sort` int(11) NULL DEFAULT 1,
  `type` int(11) NULL DEFAULT 0 COMMENT '-1未开启逐仓 0开启逐仓',
  `zcstopscale` decimal(18, 4) NULL DEFAULT 30.0000 COMMENT '逐仓自动平仓usdt金额',
  `zcscale` decimal(18, 4) NULL DEFAULT 1.0000 COMMENT '逐仓奖励倍数',
  `zctax` decimal(18, 5) NULL DEFAULT 0.10000 COMMENT '逐仓开仓手续费',
  PRIMARY KEY (`coin`) USING BTREE
) ENGINE = InnoDB CHARACTER SET = utf8mb4 COLLATE = utf8mb4_general_ci ROW_FORMAT = Compact;

