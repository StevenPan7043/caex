-- ----------------------------
-- Table structure for c_usdt_detail
-- ----------------------------
DROP TABLE IF EXISTS `c_usdt_detail`;
CREATE TABLE `c_usdt_detail`  (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `paycode` varchar(100) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT NULL COMMENT '流水号',
  `cid` int(11) NULL DEFAULT NULL COMMENT '会员编号',
  `typeid` int(11) NULL DEFAULT NULL COMMENT '操作类型见枚举类',
  `isout` int(1) NULL DEFAULT NULL COMMENT '1收入 0支出',
  `original` decimal(18, 4) NULL DEFAULT 0.0000 COMMENT '上一次值',
  `cost` decimal(18, 4) NULL DEFAULT 0.0000 COMMENT '本次操作值',
  `last` decimal(18, 4) NULL DEFAULT 0.0000 COMMENT '最后值',
  `createtime` datetime(0) NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
  `updatetime` datetime(0) NULL DEFAULT NULL COMMENT '修改时间 用来做乐观锁',
  `targetid` int(11) NULL DEFAULT NULL COMMENT '目标用户id',
  `remark` varchar(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT NULL COMMENT '备注',
  PRIMARY KEY (`id`) USING BTREE,
  INDEX `cid`(`cid`) USING BTREE
) ENGINE = InnoDB AUTO_INCREMENT = 1 CHARACTER SET = utf8mb4 COLLATE = utf8mb4_general_ci COMMENT = 'Usdt资金明细记录表' ROW_FORMAT = Compact;

-- ----------------------------
-- Table structure for c_wallet
-- ----------------------------
DROP TABLE IF EXISTS `c_wallet`;
CREATE TABLE `c_wallet`  (
  `cid` int(11) NOT NULL,
  `type` varchar(10) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NOT NULL COMMENT '钱包类型 见枚举 EOS ETH USDT BTH ',
  `addr` varchar(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT NULL COMMENT '钱包地址',
  `path` varchar(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT NULL COMMENT '文件路径 ecr20的钱包路径',
  `label` varchar(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT NULL COMMENT '标签 修改为ecr20的钱包地址',
  `password` varchar(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT NULL COMMENT '密码',
  `pubkey` varchar(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT NULL COMMENT '公钥',
  `prikey` varchar(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT NULL COMMENT '私钥',
  `balance` decimal(24, 8) NULL DEFAULT 0.00000000 COMMENT '钱包余额',
  `zcbalance` decimal(24, 8) NULL DEFAULT 0.00000000 COMMENT '逐仓金额',
  `nodeid` varchar(2) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT '1' COMMENT '节点编号',
  `qrurl` varchar(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT NULL COMMENT '二维码',
  `ecrqr` varchar(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL DEFAULT NULL COMMENT 'erc20协议二维码',
  `version` int(11) NULL DEFAULT 1 COMMENT '版本号',
  PRIMARY KEY (`cid`, `type`) USING BTREE,
  INDEX `cid`(`cid`) USING BTREE,
  INDEX `type`(`type`) USING BTREE
) ENGINE = InnoDB CHARACTER SET = utf8mb4 COLLATE = utf8mb4_general_ci COMMENT = '会员钱包信息' ROW_FORMAT = Compact;

